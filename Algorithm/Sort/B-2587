import java.util.Scanner;
import java.util.Arrays;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int[] arr = new int[5];
        int hap = 0;
        
        for(int i=0; i<5; i++) {
            int n = sc.nextInt();
            arr[i] = n;
            hap += n;
       }
       
        Arrays.sort(arr);
        
        System.out.println(hap/5);
        System.out.println(arr[2]);
    }
}
