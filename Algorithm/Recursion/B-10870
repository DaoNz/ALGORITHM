import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int[] P = new int[N+2];
        P[0] = 0;
        P[1] = 1;
        
        if (N == 0) System.out.print("0");
        else if (N == 1) System.out.print("1");
        else {
            for(int i=2; i<=N; i++) {
                P[i] = P[i-2] + P[i-1];
            }
            System.out.print(P[N]);
        }
    }
}
