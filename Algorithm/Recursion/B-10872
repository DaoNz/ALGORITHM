import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int R = 1;
        if (N==0) {
            System.out.print("1");
        } else {
            for(int i=1; i<=N; i++)
                R = R*i;
            System.out.print(R);

        }
    }  
}
